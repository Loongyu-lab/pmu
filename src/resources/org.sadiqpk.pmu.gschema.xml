<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="pmu">
  <schema id="org.sadiqpk.pmu" path="/org/sadiqpk/pmu/">
    <key name="first-run" type="b">
      <default>true</default>
      <summary>First run of PMU</summary>
      <description>Whether to do PMU intial setup or not</description>
    </key>
    <key name="pmu-id" type="u">
      <range min="1" max="65535"/>
      <default>1</default>
      <summary>An Identifier for the PMU</summary>
    </key>
    <key name="station-name" type="s">
      <default>"NIT Calicut"</default>
      <summary>Human readable name</summary>
    </key>
    <key name="admin-ip" type="s">
      <default>"192.168.1.1"</default>
      <summary>IP Address of the Admin Client</summary>
    </key>
    <key name="port" type="u">
      <range min="1025" max="65535"/>
      <default>4713</default>
      <summary>Port to use for PMU server</summary>
    </key>
  </schema>
</schemalist>
